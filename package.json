{
  "name": "electron-quick-start",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "rebuild": "electron-rebuild",
    "start": "electron .",
    "build": "npm run rebuild && electron-packager . GitLever --all -all --electron-version=1.7.4",
    "build:darwin": "npm run rebuild && electron-packager . GitLever --platform=darwin --arch=x64 --electron-version=1.7.4"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "license": "CC0-1.0",
  "devDependencies": {
    "devtron": "^1.4.0",
    "electron": "^1.6.6",
    "electron-packager": "^8.7.2",
    "electron-rebuild": "^1.5.11"
  },
  "dependencies": {
    "jquery": "^3.2.1",
    "node-notifier": "^5.1.2",
    "serialport": "^4.0.7"
  }
}
